{
    "name": "astronphp/framework",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "support": {
        "issues": "https://github.com/astronphp/framework/issues",
        "source": "https://github.com/astronphp/framework/"
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "php-coveralls/php-coveralls": "^2"
    },
    "autoload": {
        "psr-4": {
            "Astronphp\\": [
                "src/",
                "tests/"
            ]
        }
    }
}
